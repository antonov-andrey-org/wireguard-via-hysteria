acme:
  domains:
    - wireguard.example.com
  email: "admin@example.com"
  ca: letsencrypt
  dir: "/acme"
  type: tls

auth:
  type: password
  password: "mysecretpassword"

masquerade:
  type: proxy
  listenHTTP: :80
  listenHTTPS: :443
  proxy:
    url: http://wireguard-ui:80
    rewriteHost: false

outbounds:
  - name: wireguard
    type: direct

acl:
  inline:
    - wireguard(all, udp/51820)
    - direct(all)
